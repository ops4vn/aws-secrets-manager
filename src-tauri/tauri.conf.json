{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Secrets Manager",
  "version": "0.0.15",
  "identifier": "com.zed.secrets-manager",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "",
        "width": 1200,
        "height": 900,
        "maximized": true,
        "visible": false,
        "fileDropEnabled": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDU1M0QyM0VGNEMyODIxMDUKUldRRklTaE03eU05VlJmSU44clp2MHRyMlV3MjhFakkxQkgvNHpJRmZTcEY4dzZoR2F5SzE5YVYK",
      "endpoints": [
        "https://secrets-manager.dung.io.vn/releases/{{target}}/{{arch}}/latest.json"
      ]
    }
  }
}
